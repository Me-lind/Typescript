#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Dev/teach2give/Typescript/node_modules/.pnpm/@total-typescript+exercise-cli@0.4.0_typescript@5.2.2_vitest@0.34.6_jsdom@20.0.3_/node_modules/@total-typescript/exercise-cli/dist/node_modules:/mnt/c/Dev/teach2give/Typescript/node_modules/.pnpm/@total-typescript+exercise-cli@0.4.0_typescript@5.2.2_vitest@0.34.6_jsdom@20.0.3_/node_modules/@total-typescript/exercise-cli/node_modules:/mnt/c/Dev/teach2give/Typescript/node_modules/.pnpm/@total-typescript+exercise-cli@0.4.0_typescript@5.2.2_vitest@0.34.6_jsdom@20.0.3_/node_modules/@total-typescript/node_modules:/mnt/c/Dev/teach2give/Typescript/node_modules/.pnpm/@total-typescript+exercise-cli@0.4.0_typescript@5.2.2_vitest@0.34.6_jsdom@20.0.3_/node_modules:/mnt/c/Dev/teach2give/Typescript/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Dev/teach2give/Typescript/node_modules/.pnpm/@total-typescript+exercise-cli@0.4.0_typescript@5.2.2_vitest@0.34.6_jsdom@20.0.3_/node_modules/@total-typescript/exercise-cli/dist/node_modules:/mnt/c/Dev/teach2give/Typescript/node_modules/.pnpm/@total-typescript+exercise-cli@0.4.0_typescript@5.2.2_vitest@0.34.6_jsdom@20.0.3_/node_modules/@total-typescript/exercise-cli/node_modules:/mnt/c/Dev/teach2give/Typescript/node_modules/.pnpm/@total-typescript+exercise-cli@0.4.0_typescript@5.2.2_vitest@0.34.6_jsdom@20.0.3_/node_modules/@total-typescript/node_modules:/mnt/c/Dev/teach2give/Typescript/node_modules/.pnpm/@total-typescript+exercise-cli@0.4.0_typescript@5.2.2_vitest@0.34.6_jsdom@20.0.3_/node_modules:/mnt/c/Dev/teach2give/Typescript/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin.js" "$@"
else
  exec node  "$basedir/../../dist/bin.js" "$@"
fi
